  label=<
    <TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
      <TR><TD><B>src_thread</B></TD></TR>
      <TR><TD PORT="sb_emit_counter"><FONT COLOR="red"><B>sb_emit_counter</B></FONT></TD></TR>
